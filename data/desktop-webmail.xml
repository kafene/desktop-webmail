<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE default-apps SYSTEM "gnome-da-list.dtd">
<default-apps>
  <mail-readers>
    <mail-reader>
      <name>Desktop Webmail</name>
      <executable>desktop-webmail</executable>
      <command>desktop-webmail %s</command>
      <icon-name>desktop-webmail</icon-name>
      <run-in-terminal>false</run-in-terminal>
      <netscape-remote>false</netscape-remote>
    </mail-reader>
  </mail-readers>
</default-apps>
