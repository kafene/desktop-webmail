<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE default-apps SYSTEM "gnome-da-list.dtd">
<default-apps>
  <mail-readers>
    <mail-reader>
      <name>GMail</name>
      <executable>desktop-webmail --type=GMAIL</executable>
      <command>desktop-webmail-mailto --type=GMAIL %s</command>
      <icon-name>mail-emblem</icon-name>
      <run-in-terminal>false</run-in-terminal>
      <netscape-remote>false</netscape-remote>
    </mail-reader>
  </mail-readeres>
</default-apps>
